            <th class="total-th">合計</th>
            <th class="total-th">平均</th>
            <th class="total-th">成約率</th>
          </thead>
          <tbody v-for=" i in 10 ">
          <tr>
            <th rowspan="2" class="total-th">{{ i + 9 }}時</th>
            <th class="total-th">訪問</th>
            <td class="total-td">{{ hourVisit[i - 1] }}</td>
