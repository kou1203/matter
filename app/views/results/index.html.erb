              <td class=""><%= @dmer_today_data.where(user: {base: base}).length %></td>
              <td class=""><%= @aupay_today_data.where(user: {base: base}).length %></td>
              <td class=""><%= @paypay_today_data.where(user: {base: base}).length %></td>
