              <td class=""><%= @result_today_data.sum(:profit).to_s(:delimited) %></td>
              <td class=""><%= @result_today_data.average(:profit).round().to_s(:delimited) rescue "測定不可" %></td>
            </tr>
          </tbody>
        </table>
