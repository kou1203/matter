<div data-scope-path="results/edit">
  <h1>SS終着編集</h1>
  <%= render "shared/header" %>
  <%= form_with model: @result,  class: "form-frame", local: true do |f| %>
    <%= render "shared/error_messages", model: f.object %>
  <%# 基本情報 %>
    <h1>基本情報　拠点：<%= @result.user.base + @result.user.base_sub%>　<%= @result.user.name %></h1>
    <table>
      <tr>
        <th>
          <div class="form-interval">
          <%= f.label :date, "日付", class: "form-text" %>
          <%= f.date_field :date , class: "form-input" %>
          </div>
        </th>

        <th>
          <div class="form-interval">
          <%= f.label :ojt, "帯同者", class: "form-text" %>
          <%= f.text_field :ojt,class: "form-input" %>
          </div>
        </th>
      </tr>
      <tr>
        <th>
          <div class="form-interval">
          <%= f.label :area, "地域", class: "form-text" %>
          <%= f.text_field :area , class: "form-input" %>
          </div>
        </th>

        <th>
          <div class="form-interval">
          <%= f.label :shift, "シフト", class: "form-text" %>
          <%= f.select :shift, {"キャッシュレス新規": "キャッシュレス新規", "キャッシュレス決済": "キャッシュレス決済","フェムト": "フェムト", "フェムト（設置）": "フェムト（設置）", "パンダ": "パンダ", "帯同": "帯同","内勤": "内勤", "休み": "休み","欠勤": "欠勤"},{}, {class: "form-input"} %>
          </div>
        </th>

        <th>
          <%= f.label :profit, "売上", class: "form-text" %>
          <%= f.number_field :profit,class: "form-input"%>
        </th>
      </tr>
    </table>
